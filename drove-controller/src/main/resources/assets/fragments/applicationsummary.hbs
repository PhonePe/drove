
<table class="table table-bordered data-table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Expected Instances</th>
            <th>Healthy Instances</th>
            <th>CPU</th>
            <th>Memory (MB)</th>
            <th>State</th>
        </tr>
    </thead>
    <tbody>
        {{#with app}}
            <tr>
                <td>{{id}}</td>
                <td>{{#with spec}}{{name}}{{/with}}</td>
                <td>{{requiredInstances}}</td>
                <td>{{healthyInstances}}</td>
                <td>{{totalCPUs}}</td>
                <td>{{totalMemory}}</td>
                <td>{{state}}</td>
            </tr>
        {{/with}}
    </tbody>
</table>